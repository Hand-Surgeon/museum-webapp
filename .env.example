# Data Source Configuration
# Options: 'supabase' | 'json'
VITE_DATA_SOURCE=json

# Supabase Configuration (required if VITE_DATA_SOURCE=supabase)
VITE_SUPABASE_URL=your_supabase_url_here
VITE_SUPABASE_ANON_KEY=your_supabase_anon_key_here

# Optional: Use local JSON as fallback when Supabase fails
VITE_USE_LOCAL_FALLBACK=true

# Performance Monitoring (optional)
VITE_ENABLE_PERFORMANCE_MONITORING=false

# Analytics (optional)
VITE_ENABLE_ANALYTICS=false

# API Configuration (optional)
VITE_API_TIMEOUT=30000
VITE_API_RETRY_COUNT=3

# 국립박물관 API 서비스 키 (optional)
# https://www.emuseum.go.kr/openapi 에서 발급받은 API 키를 입력하세요
MUSEUM_API_KEY=your_api_key_here